{
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
             "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "semi": 2,
        "no-cond-assign": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-unreachable": 2,
        "default-case": 2,
        "no-fallthrough": 2,
        "no-self-compare": 2
    }
}
